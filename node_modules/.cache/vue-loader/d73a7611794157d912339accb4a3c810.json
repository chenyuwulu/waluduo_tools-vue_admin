n        </template>\r\n    </span>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\nconst path = process.env.VUE_APP_BASE_API\r\nexport default {\r\n    name: \"customPic\",\r\n    props: {\r\n        picType: {\r\n            type: String,\r\n            required: false,\r\n            default: \"avatar\"\r\n        },\r\n        picSrc: {\r\n            type: String,\r\n            required: false,\r\n            default: \"\"\r\n        }\r\n    },\r\n    data(){\r\n        return{\r\n            path: path,\r\n        }\r\n    },\r\n    computed:{\r\n        ...mapGetters('user', ['userInfo']),\r\n        avatar(){\r\n            if(this.picSrc === ''){\r\n                if(this.userInfo.headerImg !== '' && this.userInfo.headerImg.slice(0, 4) === \"http\"){\r\n                    return this.userInfo.headerImg\r\n                }\r\n                return this.path + this.userInfo.headerImg\r\n            }else{\r\n                if(this.picSrc !== '' && this.picSrc.slice(0, 4) === \"http\"){\r\n                    return this.picSrc\r\n                }\r\n                return this.path + this.picSrc\r\n            }\r\n        },\r\n        file(){\r\n            if(this.picSrc && this.picSrc.slice(0, 4) !== \"http\"){\r\n                return this.path + this.picSrc\r\n            }\r\n            return this.picSrc\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.headerAvatar{\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.file{\r\n    width: 80px;\r\n    height: 80px;\r\n    position: relative;\r\n}\r\n</style>"]}]}